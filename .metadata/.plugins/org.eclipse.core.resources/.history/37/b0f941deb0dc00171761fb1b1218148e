#include <iostream>
using namespace std;
 class A{
 public:
	int a;
	A(){a=100;}

	void as(){}



 };

 class B : protected A{

 public :
	 int a;

  void haha(){

	  as();
	  cout<<a;}
 };




int main() {

	A obja;

	B* bp = dynamic_cast<B*>(&obja);

	if(bp == NULL){
		cout<<"gg"
	}


}
