#include <iostream>
using namespace std;
 class A{
 public:
	int a;
	A(){a=100;}

	void as(){}



 };

 class B : protected A{

 public :

  void haha(){

	  as();
	  cout<<a;}
 };



int main() {


	B* b  = new B;
	b->as();



}
