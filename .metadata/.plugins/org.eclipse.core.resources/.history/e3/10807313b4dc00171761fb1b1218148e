#include <iostream>
using namespace std;
 class A{
 public:
	int a;
	A(){a=100;}

	virtual void haha(){}



 };

 class B : public A{

 public :
	 int a;



class C : public B{

	 void haha(){
		 cout<<"haha in C"<<endl;
	 }
 };


int main() {

	B* p = new C;
	p->haha();


}
